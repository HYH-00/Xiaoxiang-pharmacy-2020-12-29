<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.Dao.Chart.DrugstypeMapper">
    <insert id="insertDrugstype">
        insert into drugstype(id,type) values (0,#{type})
    </insert>
    <select id="getAllDrugstype" resultType="com.example.demo.Bean.DataAnalysis.Drugstype">
        select * from drugstype;
    </select>
</mapper>